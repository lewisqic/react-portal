{
  "extends": "react",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "_": false,
    "axios": false,
    "api": false
  },
  "rules": {
    "space-before-function-paren": "off",
    "space-in-parens": "off",
    "eol-last": "off",
    "padded-blocks": "off",
    "no-trailing-spaces": "off",
    "no-unused-vars": "off",
    "max-len": "off",
    "react/prop-types": "off",
    "no-multiple-empty-lines": "off",
    "no-use-before-define": "off",
    "react/sort-comp": "off"
  }
}